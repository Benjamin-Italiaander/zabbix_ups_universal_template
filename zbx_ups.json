                    {
                        "uuid": "dc03c6321bc743f2ac6dea6e8171a6f8",
                        "name": "Battery capacity",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.2.1.0",
                        "key": "battery.capacity[BatteryCapacity]",
                        "delay": "5m",
                        "units": "%",
                        "description": "MIB: PowerNet-MIB\nThe remaining battery capacity expressed as\n percentage of full capacity."
                    },
                    {
                        "uuid": "06d725772c624b9cbd4cdf87f10c5dc2",
                        "name": "Battery last replace date",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.1.3.0",
                        "key": "battery.last_replace_date[upsBasicBatteryLastReplaceDate]",
                        "delay": "1d",
                        "trends": "0",
                        "value_type": "CHAR",
                        "description": "MIB: PowerNet-MIB\nThe date when the UPS system's batteries were last replaced\n in mm/dd/yy (or yyyy) format. For Smart-UPS models, this value\n is originally set at the factory. When the UPS batteries\n are replaced, this value should be reset by the administrator.\n For Symmetra PX 250/500 this OID is read-only and is configurable in the local display only."
                    },
                    {
                        "uuid": "c85ef5a894034e5ba81167e1171b8cba",
                        "name": "Battery replace",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.2.4.0",
                        "key": "battery.replace[upsAdvBatteryReplace]",
                        "description": "If result = 2 then battery needs replacing (1 = ok)",
                        "valuemap": {
                            "name": "Battery needs to be replaced"
                        }
                    },
                    {
                        "uuid": "56c7f683622a4b9b8848b3705d18b306",
                        "name": "Battery Time Remaining",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.2.3.0",
                        "key": "battery.status[upsAdvBatteryRunTimeRemaining]",
                        "value_type": "FLOAT",
                        "units": "minutes",
                        "description": "The UPS battery run time remaining before battery exhaustion.",
                        "preprocessing": [
                            {
                                "type": "MULTIPLIER",
                                "parameters": [
                                    "0.000166666667"
                                ]
                            }
                        ]
                    },
                    {
                        "uuid": "98bfd87bfa744e908c0fe3f99fb4c1eb",
                        "name": "Battery status",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.1.1.0",
                        "key": "battery.status[upsBasicBatteryStatus]",
                        "description": "The status of the UPS batteries. A batteryLow(3) value indicates the UPS will be unable to sustain the current load, and its services will be lost if power is not restored. The amount of run time in reserve at the time of low battery can be configured by the upsAdvConfigLowBatteryRunTime. Enumeration: 'unknown': 1, 'batteryNormal': 2, 'batteryLow': 3.\n installed has an internal error condition.",
                        "valuemap": {
                            "name": "Battery status"
                        }
                    },
                    {
                        "uuid": "710767bacb2c4b7eb607e4ef1b9435b3",
                        "name": "Battery temperature",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.2.2.0",
                        "key": "battery.temperature[upsHighPrecBatteryTemperature]",
                        "value_type": "FLOAT",
                        "units": "\u2103",
                        "triggers": [
                            {
                                "uuid": "7d8462bbcd9148088312c2e6917812a2",
                                "expression": "last(/APC simple template/battery.temperature[upsHighPrecBatteryTemperature])>30",
                                "name": "Battery temperature above 30",
                                "priority": "DISASTER"
                            }
                        ]
                    },
                    {
                        "uuid": "e249507e5af74faf8f7c25e1992a20ef",
                        "name": "Battery voltage",
                        "type": "SNMP_AGENT",
                        "snmp_oid": "1.3.6.1.4.1.318.1.1.1.2.2.8.0",
